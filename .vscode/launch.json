  "version": "0.2.0",
  "configurations": [
    {
      "cwd": "${workspaceFolder}/netconf_client",
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/netconf_client/main.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      // "args": ["--netconf_filter", "cisco_xe_ietf-interfaces.xml"]
      "args": [
        "--xml_filter",
        // "cisco_xe_ietf-interfaces.xml",
        "Cisco-IOS-XE-interfaces-oper.xml",
        // "--xpath_filter",
        // "http://cisco.com/ns/yang/Cisco-IOS-XE-interfaces-oper:interfaces/interface",
        // "interfaces/interface",
        "--device_settings",
        "devnet_xe_sandbox_settings.json"
      ]
    }
  ]
}
